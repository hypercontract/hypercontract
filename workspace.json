{
  "version": 1,
  "projects": {
    "express": {
      "root": "libs/express",
      "sourceRoot": "libs/express/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/express/**/*.ts",
              "libs/express/**/*.spec.ts",
              "libs/express/**/*.spec.tsx",
              "libs/express/**/*.spec.js",
              "libs/express/**/*.spec.jsx",
              "libs/express/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/express/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/express"
          ]
        }
      },
      "tags": []
    },
    "hypercontract": {
      "root": "apps/hypercontract",
      "sourceRoot": "apps/hypercontract/src",
      "projectType": "application",
      "prefix": "hypercontract",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:webpack",
          "options": {
            "outputPath": "dist/apps/hypercontract",
            "main": "apps/hypercontract/src/main.ts",
            "tsConfig": "apps/hypercontract/tsconfig.app.json",
            "assets": [
              "apps/hypercontract/src/assets"
            ],
            "buildLibsFromSource": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/hypercontract/src/environments/environment.ts",
                  "with": "apps/hypercontract/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@nrwl/node:node",
          "options": {
            "buildTarget": "hypercontract:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/hypercontract/**/*.ts",
              "apps/hypercontract/**/*.spec.ts",
              "apps/hypercontract/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/hypercontract/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/hypercontract"
          ]
        }
      },
      "tags": []
    },
    "hypershop": {
      "root": "apps/hypershop",
      "sourceRoot": "apps/hypershop/src",
      "projectType": "application",
      "prefix": "hypershop",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:webpack",
          "options": {
            "outputPath": "dist/apps/hypershop",
            "main": "apps/hypershop/src/main.ts",
            "tsConfig": "apps/hypershop/tsconfig.app.json",
            "assets": [
              "apps/hypershop/src/assets",
              "apps/hypershop/src/app/formats/html",
              "apps/hypershop/src/app/api-root/templates",
              "apps/hypershop/src/app/products/templates",
              "apps/hypershop/src/app/shopping-cart/templates",
              "apps/hypershop/src/app/user-profile/templates",
              "apps/hypershop/src/app/orders/templates"
            ],
            "buildLibsFromSource": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/hypershop/src/environments/environment.ts",
                  "with": "apps/hypershop/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@nrwl/node:node",
          "options": {
            "buildTarget": "hypershop:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/hypershop/**/*.ts",
              "apps/hypershop/**/*.spec.ts",
              "apps/hypershop/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/hypershop/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/hypershop"
          ]
        }
      },
      "tags": []
    },
    "profile": {
      "root": "libs/profile",
      "sourceRoot": "libs/profile/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/profile/**/*.ts",
              "libs/profile/**/*.spec.ts",
              "libs/profile/**/*.spec.tsx",
              "libs/profile/**/*.spec.js",
              "libs/profile/**/*.spec.jsx",
              "libs/profile/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/profile/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/profile"
          ]
        }
      },
      "tags": []
    }
  }
}
